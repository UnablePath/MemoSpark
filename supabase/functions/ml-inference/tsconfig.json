{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["deno.ns", "deno.window", "dom", "esnext"],
    "types": ["https://esm.sh/@supabase/functions-js/src/edge-runtime.d.ts"],
    "jsx": "react-jsx",
    "allowJs": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      // "@/*": ["../../src/*"]
    }
  },
  "include": ["./**/*.ts"],
  "exclude": ["../../node_modules"]
} 